<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwipeRefreshLayout">
        <attr name="srl_direction">
            <enum name="disable" value="-1"/>
            <enum name="top" value="0"/>
            <enum name="bottom" value="1"/>
            <enum name="both" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="MultiSwipeRefreshLayout">
        <attr name="foreground" format="reference"/>
    </declare-styleable>
</resources>
